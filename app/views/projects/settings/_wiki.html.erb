<% unless params[:controller] == 'my' %>
  <%= labelled_form_for @wiki,
                     :as => :wiki,
                     :url => { :controller => 'wikis', :action => 'edit', :id => @project },
                     :html => {:id => 'edit_wiki'},
                     :remote => true, :method => 'post' do |f| %>

    <%= error_messages_for 'wiki' %>
    
    <%= render :partial => 'projects/settings/wiki_content'%>

  <% end %>
<% else %>
  <%= render :partial => 'projects/settings/wiki_content'%>  
<% end %>